<extension xmlns="http://ns.adobe.com/air/extension/25.0">
	<id>com.myflashlab.air.extensions.cmd</id>
	<versionNumber>1.0.1</versionNumber>
	<copyright>2018, MyFlashLabs Team. All rights reserved.</copyright>
	<description>
		<text xml:lang="en">Air Native Extension!</text>
	</description>
	<platforms>

		<platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>classes.jar</nativeLibrary>
				<initializer>com.myflashlab.cmd.MyExtension</initializer>
				<finalizer>com.myflashlab.cmd.MyExtension</finalizer>
			</applicationDeployment>
		</platform>

		<platform name="Android-x86">
			<applicationDeployment>
				<nativeLibrary>classes.jar</nativeLibrary>
				<initializer>com.myflashlab.cmd.MyExtension</initializer>
				<finalizer>com.myflashlab.cmd.MyExtension</finalizer>
			</applicationDeployment>
		</platform>

		<platform name="iPhone-ARM">
			<applicationDeployment>
				<!-- <nativeLibrary>libexCMD.a</nativeLibrary>
			<initializer>comMyflashlabsCMDMyExtensionInitializer</initializer>
			<finalizer>comMyflashlabsCMDMyExtensionFinalizer</finalizer> -->
			</applicationDeployment>
		</platform>

		<platform name="default">
			<applicationDeployment/>
		</platform>

	</platforms>
</extension>

